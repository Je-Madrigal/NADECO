@using DataLibrary.Models

@model List<Members>

 

<div class="pcoded-content">
    <!-- [ breadcrumb ] start -->
    <div class="page-header">
        <div class="page-block">
            <div class="row align-items-center">
                <div class="col-md-12">
                    <div class="page-header-title">
                        <h5 class="m-b-10">Hello! Administrator</h5>
                    </div>
                    <ul class="breadcrumb">
                        <li class="breadcrumb-item"><a href="@Url.Action("Admin_Dashboard", "Home")"><i class="feather icon-home"></i></a></li>

                        <li class="breadcrumb-item"><a href="@Url.Action("Index", "Members")">Members Information Lists</a></li>
                    </ul>
                </div>
            </div>
        </div>
        <hr />
        <div class="row">
            <div class="col-xl-12">
                <div class="card">
                    <div class="card-header d-flex justify-content-between align-items-center">
                        <h5>Members Masterlist</h5>
                        <button class="btn  btn-primary" id="BtnAddNewMember"><i class="feather mr-2 icon-file-plus"></i>Add member</button>

                    </div>

                    <div class="card-body table-border-style">
                        <div class="table-responsive">
                            <table class="myTable table table-striped table-bordered">
                                <thead>
                                    <tr>
                                        <th>No</th>

                                        <th>Entry Date</th>
                                        <th>FullName</th>

                                        <th>TIN</th>
                                        <th>Address</th>

                                        <th>Account Status</th>
                                        <th>Registration Status</th>
                                        @* <th>Action</th>*@
                                        @*<th>ImageFileName</th>*@

                                    </tr>
                                </thead>
                                <tbody style="word-wrap:break-word">
                                    @foreach (var members in Model)
                                    {
                                        <tr style="text-align: center; vertical-align: middle;">
                                            <td><a href="@Url.Action("Profile_", "Members", new { No_ = members.No_ })">@members.No_</a></td>
                                            <td>@members.EntryDate.ToShortDateString()</td>
                                            <td>@members.FullName</td>

                                            <td>@members.TINNo</td>
                                            <td>@members.FullAddress</td>

                                            <td>@members.AccountStatus</td>
                                            <td>@members.RegistrationStatus</td>
                                            <!--<td class="responsive-td" style=" align-items: center;">-->
                                            @*<a href="" onclick="return LoadFile('@Url.Content("~/" + rpt.ImageFileName)')" class="btn btn-outline-danger me-2" style="flex: 1; font-size: smaller; margin: 5px;"><i class="fa fa-eye me-1"></i>VIEW </a>

        <a href="@Url.Content("~/" + rpt.ImageFileName)" target="_blank" class="btn btn-outline-success" style="flex: 1; font-size: smaller;"><i class="fa fa-print me-1"></i>PRINT</a>*@
                                            <!--</td>-->




                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>